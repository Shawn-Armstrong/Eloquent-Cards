{"ast":null,"code":"import Theme1 from \"../assets/Card-theme-1-Back.jpg\";\nimport Theme2 from \"../assets/Card theme-2-Back.jpg\";\nimport Theme3 from \"../assets/card theme 3-Back2.jpg\";\nimport Theme1F from \"../assets/Card theme-1-Front.jpg\";\nimport Theme2F from \"../assets/Card theme-2-Front.jpg\";\nimport Theme3F from \"../assets/card theme 3-front2.jpg\";\nexport default {\n  name: \"Templates\",\n\n  data() {\n    return {\n      templates: [{\n        name: \"Template 1\",\n        src: Theme1,\n        theme: \"1\",\n        src2: Theme1F\n      }, {\n        name: \"Template 2\",\n        src: Theme2,\n        theme: \"2\",\n        src2: Theme2F\n      }, {\n        name: \"Template 3\",\n        src: Theme3,\n        theme: \"3\",\n        src2: Theme3F\n      }]\n    };\n  },\n\n  methods: {\n    Choice(theme) {\n      this.$emit(\"choosetemplate\", true);\n      this.$emit(\"templatetype\", theme);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8DA,OAAOA,MAAP,MAAmB,iCAAnB;AACA,OAAOC,MAAP,MAAmB,iCAAnB;AACA,OAAOC,MAAP,MAAmB,kCAAnB;AACA,OAAOC,OAAP,MAAoB,kCAApB;AACA,OAAOC,OAAP,MAAoB,kCAApB;AACA,OAAOC,OAAP,MAAoB,mCAApB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEF,IAAI,EAAE,YAAR;QAAsBG,GAAG,EAAET,MAA3B;QAAmCU,KAAK,EAAE,GAA1C;QAA+CC,IAAI,EAAER;MAArD,CADS,EAET;QAAEG,IAAI,EAAE,YAAR;QAAsBG,GAAG,EAAER,MAA3B;QAAmCS,KAAK,EAAE,GAA1C;QAA+CC,IAAI,EAAEP;MAArD,CAFS,EAGT;QAAEE,IAAI,EAAE,YAAR;QAAsBG,GAAG,EAAEP,MAA3B;QAAmCQ,KAAK,EAAE,GAA1C;QAA+CC,IAAI,EAAEN;MAArD,CAHS;IADN,CAAP;EAOD,CAVY;;EAWbO,OAAO,EAAE;IACPC,MAAM,CAACH,KAAD,EAAQ;MACZ,KAAKI,KAAL,CAAW,gBAAX,EAA6B,IAA7B;MACA,KAAKA,KAAL,CAAW,cAAX,EAA2BJ,KAA3B;IACD;;EAJM;AAXI,CAAf","names":["Theme1","Theme2","Theme3","Theme1F","Theme2F","Theme3F","name","data","templates","src","theme","src2","methods","Choice","$emit"],"sourceRoot":"","sources":["/Users/stevenbui/Desktop/Eloquent-Cards-Steven/Client/src/components/Templates.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"templates\">\n      <h1 class=\"subheading\">Choose a Template</h1>\n      <v-container class=\"card-templates\">\n        <v-layout row wrap justify-center>\n          <v-flex\n            xs12\n            sm6\n            md4\n            lg3\n            v-for=\"template in templates\"\n            :key=\"template.name\"\n          >\n            <v-hover>\n              <template v-slot=\"{ hover }\">\n                <v-card\n                  class=\"text-xs-center ma-3\"\n                  :elevation=\"hover ? 15 : 6\"\n                  color=\"#fff\"\n                  :src=\"template.src\"\n                >\n                  <v-responsive class=\"pt-0\">\n                    <div class=\"flip-card-container\">\n                      <div class=\"flip-card\">\n                        <div class=\"flip-card-front\">\n                          <v-img :src=\"template.src\"></v-img>\n                        </div>\n                        <div class=\"flip-card-back\">\n                          <v-img :src=\"template.src2\"></v-img>\n                        </div>\n                      </div>\n                    </div>\n                  </v-responsive>\n                  <v-card-text class=\"pb-0\">\n                    <div class=\"text--primary\">\n                      <h3>{{ template.name }}</h3>\n                    </div>\n                  </v-card-text>\n                  <v-card-actions>\n                    <v-layout>\n                      <v-btn\n                        flat\n                        large\n                        color=\"blue\"\n                        @click=\"Choice(template.theme)\"\n                      >\n                        Choose</v-btn\n                      >\n                    </v-layout>\n                  </v-card-actions>\n                </v-card>\n              </template>\n            </v-hover>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </div>\n  </v-app>\n</template>\n\n<script>\nimport Theme1 from \"../assets/Card-theme-1-Back.jpg\";\nimport Theme2 from \"../assets/Card theme-2-Back.jpg\";\nimport Theme3 from \"../assets/card theme 3-Back2.jpg\";\nimport Theme1F from \"../assets/Card theme-1-Front.jpg\";\nimport Theme2F from \"../assets/Card theme-2-Front.jpg\";\nimport Theme3F from \"../assets/card theme 3-front2.jpg\";\n\nexport default {\n  name: \"Templates\",\n  data() {\n    return {\n      templates: [\n        { name: \"Template 1\", src: Theme1, theme: \"1\", src2: Theme1F },\n        { name: \"Template 2\", src: Theme2, theme: \"2\", src2: Theme2F },\n        { name: \"Template 3\", src: Theme3, theme: \"3\", src2: Theme3F },\n      ],\n    };\n  },\n  methods: {\n    Choice(theme) {\n      this.$emit(\"choosetemplate\", true);\n      this.$emit(\"templatetype\", theme);\n    },\n  },\n};\n</script>\n\n<style>\n.flip-card-container {\n  display: flex;\n  width: 405px;\n  height: 240px;\n  perspective: 1000px;\n}\n.flip-card {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  transition: transform 0.5s;\n  transform-style: preserve-3d;\n}\n.flip-card-container:hover .flip-card {\n  transform: rotateY(180deg); /* <=>  rotateY(.5turn) */\n}\n/* Position the front and back side */\n.flip-card-front,\n.flip-card-back {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  -webkit-backface-visibility: hidden; /* Safari */\n  backface-visibility: hidden;\n  /* border-radius: 0.5rem; */\n}\n\n.flip-card-back {\n  transform: rotateY(180deg);\n}\n\nv-img {\n  width: auto;\n  height: auto;\n}\n</style>"]},"metadata":{},"sourceType":"module"}